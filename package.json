{
  "name": "@i18n-circle/i18n-circle-model",
  "version": "1.0.15",
  "description": "datamodel for server and frontend usage of i18n",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "ngbuild": "ng-packagr -p ng-package.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run ngbuild",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/i18n-circle/i18n-circle-model.git"
  },
  "keywords": [
    "i18n",
    "internationalisation"
  ],
  "files": [
    "dist/**/*"
  ],
  "author": "myonara",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/i18n-circle/i18n-circle-model/issues"
  },
  "homepage": "https://github.com/i18n-circle/i18n-circle-model#readme",
  "devDependencies": {
    "@angular/compiler-cli": "^15.0.1",
    "@babel/preset-typescript": "^7.18.6",
    "@jest/globals": "^29.3.1",
    "@types/node": "^18.11.9",
    "jest": "^29.3.1",
    "ng-packagr": "^15.0.1",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.8.4"
  },
  "peerDependencies": {
    "rxjs": "^7.5.7"
  }
}
